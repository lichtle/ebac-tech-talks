n\r\n      &__item {\r\n        margin-bottom: 24px;\r\n      }\r\n    }\r\n  }\r\n\r\n  @media (max-width: 1024px) {\r\n    &__infos {\r\n      gap: 16px;\r\n    }\r\n  }\r\n}\r\n","styles\\_variables.scss":"$textColor: #fff;\r\n$secondaryTextColor: #000;\r\n$buttonColor: #4f2ac3;\r\n$gradient1: #186ca7;\r\n$gradient2: #442bdb;\r\n$gradient3: #89223b;\r\n$gradient4: #9c791d;\r\n","styles\\components\\_buttons.scss":"@use \"../variables\";\r\n\r\n.button {\r\n  padding: 12px;\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  text-decoration: none;\r\n  text-align: center;\r\n  white-space: nowrap; // Forçando o texto do botão a ficar em uma linha só\r\n\r\n  &--primary {\r\n    color: variables.$textColor;\r\n    background-color: variables.$buttonColor;\r\n  }\r\n\r\n  @media (max-width: 640px) {\r\n    display: block;\r\n    width: 100%;\r\n  }\r\n\r\n  @media (max-width: 1024px) {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n}\r\n","styles\\_event.scss":"@use \"variables\";\r\n\r\n.event {\r\n  padding: 80px 0;\r\n\r\n  &--front-end {\r\n    background-image: linear-gradient(\r\n      variables.$gradient1,\r\n      variables.$gradient2\r\n    );\r\n  }\r\n\r\n  &--back-end {\r\n    background-image: linear-gradient(\r\n      variables.$gradient2,\r\n      variables.$gradient3\r\n    );\r\n  }\r\n\r\n  &--ux-ui {\r\n    background-image: linear-gradient(\r\n      variables.$gradient3,\r\n      variables.$gradient4\r\n    );\r\n  }\r\n\r\n  &--data-science {\r\n    background-image: linear-gradient(\r\n      variables.$gradient4,\r\n      variables.$gradient3\r\n    );\r\n  }\r\n\r\n  &--image-left {\r\n    .container {\r\n      flex-direction: row-reverse;\r\n    }\r\n  }\r\n\r\n  .container {\r\n    display: flex;\r\n    gap: 40px;\r\n  }\r\n\r\n  &__details {\r\n    &__title {\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n      margin-bottom: 16px;\r\n    }\r\n\r\n    &__description {\r\n      line-height: 22px;\r\n    }\r\n  }\r\n\r\n  &__image {\r\n    max-width: 360px;\r\n  }\r\n\r\n  @media (max-width: 640px) {\r\n    .container {\r\n      display: block;\r\n    }\r\n\r\n    &__details {\r\n      margin-bottom: 16px;\r\n    }\r\n\r\n    &__image {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n","styles\\main.scss":"@use \"hero\";\r\n@use \"components/info-bar\";\r\n@use \"components/buttons\";\r\n@use \"event\";\r\n@use \"variables\";\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n  color: variables.$textColor;\r\n  list-style: none;\r\n}\r\n\r\nbody {\r\n  font-family: \"Roboto\", sans-serif;\r\n}\r\n\r\n.container {\r\n  max-width: 960px;\r\n  width: 100%;\r\n  margin: 0 auto;\r\n\r\n  @media (max-width: 640px) {\r\n    max-width: 80%;\r\n  }\r\n\r\n  @media (max-width: 1024px) {\r\n    max-width: 90%;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"fda266f6967414d35e128b71b5d5279a","cacheData":{"env":{}}}